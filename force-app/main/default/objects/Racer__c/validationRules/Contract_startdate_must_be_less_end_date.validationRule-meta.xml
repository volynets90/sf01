<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_startdate_must_be_less_end_date</fullName>
    <active>true</active>
    <description>Contract startdate must be less end date</description>
    <errorConditionFormula>Contact_End_Date__c  &lt;  Contract_Start_Date__c</errorConditionFormula>
